            <title type="text">环与域</title>
            <link rel="alternate" type="text/html" href="https://example.com/tech/%E7%8E%AF%E4%B8%8E%E5%9F%9F/" />
            <id>https://example.com/tech/%E7%8E%AF%E4%B8%8E%E5%9F%9F/</id>
